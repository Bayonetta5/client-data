<sprite>
    <imageset name="npc"
              src="graphics/sprites/npcs/sprite/artis-sewer-wheel.png"
              width="32"
              height="32"/>

    <action name="stand" imageset="npc">
        <animation direction="default">
            <frame index="0"/>
        </animation>
    </action>
    <action name="move" imageset="npc">
        <!-- Opening -->
        <animation direction="default">
            <sequence start="0" end="7" delay="50" repeat="4"/>
            <end/>
        </animation>
    </action>
</sprite>
