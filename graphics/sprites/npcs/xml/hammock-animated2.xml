<sprite>
    <imageset name="npc" src="graphics/sprites/npcs/sprite/hammock-animated2.png" width="96" height="64"/>

    <action name="stand" imageset="npc">
        <!-- Moving -->
        <animation direction="left">
            <frame index="7" delay="300"/>
            <frame index="6" delay="180"/>
            <frame index="5" delay="150"/>
            <frame index="4" delay="400"/>
            <frame index="5" delay="180"/>
            <frame index="6" delay="150"/>
        </animation>
        <!-- Static -->
        <animation direction="up">
            <frame index="7"/>
        </animation>
    </action>
</sprite>
